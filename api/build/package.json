{
  "name": "hotspotplus-api",
  "version": "4.0.2",
  "main": "server/server.js",
  "bin": "server/server.js",
  "pkg": {
    "scripts": [
      "server/boot/**/*.js",
      "node_modules/loopback/server/middleware/*.js",
      "node_modules/loopback/lib/connectors/*.js",
      "node_modules/loopback/common/**/*.js",
      "node_modules/moment/locale/*.js",
      "common/**/*.js"
    ],
    "assets": [
      "node_modules/*/node_modules/loopback-phase/intl/**/*.json",
      "node_modules/*/intl/**/*.json",
      "node_modules/*/node_modules/strong-globalize/cldr/*.json",
      "node_modules/loopback/common/**/*.json",
      "node_modules/strong-globalize/cldr/*.json",
      "node_modules/strong-remoting/node_modules/strong-error-handler/intl/**/*.json",
      "node_modules/strong-remoting/node_modules/strong-error-handler/views/*",
      "node_modules/strong-error-handler/intl/**/*.json",
      "node_modules/strong-error-handler/views/*",
      "client/dist/*",
      "common/**/*.json",
      "server/**/*.json"
    ]
  },
  "scripts": {
    "start": "node server/server.js",
    "start:dev": "nodemon ./server/server.js",
    "pretest": "eslint .",
    "build:dev:angular": "ELASTIC_IP=99 ELASTIC_PORT=9090 RABBITMQ_USERNAME=payam RABBITMQ_PASSWORD=123 lb-ng ./server/server.js  ../dashboard/src/js/lb-services.js -u http://127.0.0.1:3000/api",
    "build:prod:angular": "ELASTIC_IP=99 ELASTIC_PORT=9090 RABBITMQ_USERNAME=payam RABBITMQ_PASSWORD=123 lb-ng ./server/server.js  ../dashboard/src/js/lb-services.js",
    "build": "ELASTIC_IP=localhost ELASTIC_PORT=9200 npm run build:prod:angular && rm -rf build && grunt",
    "releaseSandbox": "grunt ;git commit -a -m 'sandbox:latest';git tag -f release ;git push -u origin develop --tags --force",
    "releaseProduction": "grunt ;git commit -a -m 'production:3.12';git tag -f release ;git push -u origin master --tags --force",
    "releaseLocalProduction": "grunt ;git commit -a -m 'production:latest';git tag -f release ;git push -u origin master --tags --force",
    "posttest": "nsp check"
  },
